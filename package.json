{
  "devDependencies": {
    "browserify-shim": "~3.2.2",
    "gulp-util": "~2.2.12",
    "gulp": "~3.4.0",
    "gulp-browserify": "~0.3.3",
    "gulp-uglify": "~0.1.0",
    "gulp-rename": "~0.2.2",
    "gulp-notify": "~0.3.4-2",
    "gulp-jshint": "~1.3.4"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./js/lib/jquery.js",
    "underscore": "./js/lib/underscore-1.5.2.js",
    "backbone": "./js/lib/backbone-1.1.0.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$",
        "underscore:_"
      ]
    }
  }
}
